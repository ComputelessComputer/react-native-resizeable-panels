#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/johnjeong/dev/Projects/react-native-resizeable-panels/node_modules/.pnpm/react-native-builder-bob@0.20.4/node_modules/react-native-builder-bob/bin/node_modules:/Users/johnjeong/dev/Projects/react-native-resizeable-panels/node_modules/.pnpm/react-native-builder-bob@0.20.4/node_modules/react-native-builder-bob/node_modules:/Users/johnjeong/dev/Projects/react-native-resizeable-panels/node_modules/.pnpm/react-native-builder-bob@0.20.4/node_modules:/Users/johnjeong/dev/Projects/react-native-resizeable-panels/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/johnjeong/dev/Projects/react-native-resizeable-panels/node_modules/.pnpm/react-native-builder-bob@0.20.4/node_modules/react-native-builder-bob/bin/node_modules:/Users/johnjeong/dev/Projects/react-native-resizeable-panels/node_modules/.pnpm/react-native-builder-bob@0.20.4/node_modules/react-native-builder-bob/node_modules:/Users/johnjeong/dev/Projects/react-native-resizeable-panels/node_modules/.pnpm/react-native-builder-bob@0.20.4/node_modules:/Users/johnjeong/dev/Projects/react-native-resizeable-panels/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../react-native-builder-bob/bin/bob" "$@"
else
  exec node  "$basedir/../react-native-builder-bob/bin/bob" "$@"
fi
